{
    "name": "idez/nova-checkboxes-field",
    "description": "A Laravel Nova Checkboxes field.",
    "keywords": [
        "laravel",
        "nova",
        "idez",
        "idez digital",
        "custom field",
        "checkbox",
        "checkboxes"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "authors": [
        {
            "name": "Rafael Laurindo",
            "email": "rafael@idez.com.br",
            "homepage": "https://idez.com.br",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/idezdigital/nova-checkboxes-field/issues"
    },
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^5.0"
    },
    "require-dev": {
        "laravel/nova-devtool": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Idez\\NovaCheckboxesField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idez\\NovaCheckboxesField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
